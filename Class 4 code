from enum import Enum
class quality(Enum):
    Excellent =1 
    good = 2
    medium = 3  
    
class Type(Enum):
    MinorService = 1
    majorService = 2

class guaranty(Enum):
    yes = 1
    no = 2

class Service:
    def __init__(self, Title, timeFactor, charges, quality, Type,guaranty):
        self.Title = Title
        self.timeFactor = timeFactor
        self.charges = charges
        self.quality = quality
        self.Type = Type
        self.guaranty = guaranty

    def set_Title(self,Title):
        self.Title = Title
    def get_Title(self):
        return self.Title
    def set_timeFactor(self,timeFactor):
        self.timeFactor = timeFactor
    def get_timeFactor(self):
        return self.timeFactor   
    def set_charges(self,charges):
        self.charges = charges
    def get_charges(self):
        return self.charges
    def set_quality(self,quality):
        self.quality = quality
    def get_quality(self):
        return self.quality
    def set_Type(self,model):
        self.Type = Type
    def get_Type(self):
        return self.Type
    def set_guaranty(self,guaranty):
        self.guaranty= guaranty
    def get_guaranty(self):
        return self.guaranty
    
        
    def __str__(self):
        return ("Title:"+self.Title+", Time Factor: "+self.timeFactor+", Charges: "+self.charges+ 
            ", Quality: "+self.quality+ ", Type: "+self.Type+ ", Guaranty: "+self.guaranty)
        
object1 = Service("Oil replacment","00:15:00","230","Good","Minor Service" ,"yes")
print(object1)
object2 =  Service("Tire replacment","00:45:00","2300","Good","Minor Service","yes")
print(object2)
